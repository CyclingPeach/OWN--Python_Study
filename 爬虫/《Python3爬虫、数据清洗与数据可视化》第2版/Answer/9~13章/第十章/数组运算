arr1 = np.arange(4)
arr2 = np.arange(10, 14)
print(arr1, '+', arr2, '=', arr1+arr2)
print(arr1, '*', arr2, '=', arr1*arr2)
#输出结果为：[0 1 2 3] * [10 11 12 13] = [ 0 11 24 39]，表示数组与数组相乘
print(1.5 * arr1)            
#输出结果为：array([ 0. ,  1.5,  3. ,  4.5])，表示数组与数字相乘
x = np.linspace(0, 2*np.pi, 20)
y = np.sin(x)